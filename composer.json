{
    "name": "dietrichxx/file-manager",
    "description": "Package for managing files and directories in a Laravel project",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dietrichxx\\FileManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "dima",
            "email": "dmitriy.pidchenko@itgroup.kz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "intervention/image-laravel": "^1.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intervention\\Image\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Image": "Intervention\\Image\\Laravel\\Facades\\Image"
            }
        }
    }
}
